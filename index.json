{
  "css": {
    "body": {
      "backgroundColor": "#f4f4f4",
      "fontFamily": "Arial, sans-serif"
    },
    ".login-container": {
      "backgroundColor": "white",
      "padding": "20px",
      "borderRadius": "8px",
      "boxShadow": "0 4px 6px rgba(0, 0, 0, 0.1)"
    }
  },
  "html": {
    "tag": "div",
    "attrs": {
      "class": "login-container"
    },
    "children": [
      {
        "tag": "h2",
        "text": "Login"
      },
      {
        "tag": "form",
        "attrs": {
          "id": "loginForm"
        },
        "children": [
          {
            "tag": "input",
            "attrs": {
              "type": "text",
              "id": "username",
              "placeholder": "Usu√°rio"
            }
          },
          {
            "tag": "input",
            "attrs": {
              "type": "password",
              "id": "password",
              "placeholder": "Senha"
            }
          },
          {
            "tag": "button",
            "attrs": {
              "type": "submit"
            },
            "text": "Entrar"
          }
        ]
      }
    ]
  },
  "js": "console.log('JavaScript carregado com sucesso!');"
}