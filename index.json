[
  {
    "elemento": "div",
    "atributos": {
      "class": "tela-login"
    },
    "filhos": [
      {
        "elemento": "h1",
        "atributos": {
          "class": "titulo"
        },
        "conteúdo": "Bem-vindo"
      },
      {
        "elemento": "form",
        "atributos": {
          "class": "formulario"
        },
        "filhos": [
          {
            "elemento": "label",
            "atributos": {
              "for": "email"
            },
            "conteúdo": "Email:"
          },
          {
            "elemento": "input",
            "atributos": {
              "type": "email",
              "id": "email",
              "name": "email",
              "placeholder": "Digite seu email"
            }
          },
          {
            "elemento": "label",
            "atributos": {
              "for": "senha"
            },
            "conteúdo": "Senha:"
          },
          {
            "elemento": "input",
            "atributos": {
              "type": "password",
              "id": "senha",
              "name": "senha",
              "placeholder": "Digite sua senha"
            }
          },
          {
            "elemento": "button",
            "atributos": {
              "type": "submit",
              "class": "botao-login"
            },
            "conteúdo": "Entrar"
          }
        ]
      }
    ]
  }
]